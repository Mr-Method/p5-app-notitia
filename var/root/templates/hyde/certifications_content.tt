<!-- Certifications -->
<div class="unit four-fifths">
   <article>
      <h4>[% page.title %]</h4>
      <form action="[% page.fields.action %]"
             class="wide-form no-header-wrap"
           enctype="application/x-www-form-urlencoded"
            method="post" name="certifications">
[% TextField( page.fields.name ) -%]
      </form>
[% Table( page.fields.certs ) -%]
[% IF page.fields.links; Dispatch( page.fields.links ); END -%]
   </article>
</div>
